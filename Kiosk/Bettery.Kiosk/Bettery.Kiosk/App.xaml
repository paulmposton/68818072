<Application x:Class="Bettery.Kiosk.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml" Startup="Application_Startup" DispatcherUnhandledException="Application_DispatcherUnhandledException">
    <Application.Resources>
        <ResourceDictionary >
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Button.xaml"/>
                <ResourceDictionary Source="Resources/TextBlock.xaml"/>
                <ResourceDictionary Source="Resources/CheckBox.xaml"/>
                <ResourceDictionary Source="Resources/RadioButton.xaml"/>
                <ResourceDictionary Source="Resources/TextBox.xaml"/>
                <ResourceDictionary Source="Resources/PasswordBox.xaml"/>
                <ResourceDictionary Source="Resources/BusyIndicator.xaml"/>
                <ResourceDictionary Source="Resources/FlowDocumentPageViewer.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
